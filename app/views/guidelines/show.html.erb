<p id="notice"><%= notice %></p>

<p>
  <b>Title:</b>
  <%= @guideline.title %>
</p>

<p>
  <b>Content:</b>
  <a href="<%= @guideline.content %>"><%= @guideline.content %></a> 
</p>

<p>
  <b>Hospital:</b>
  <%= @guideline.hospital %>
</p>

<% if @guideline.user %>
<p>
  <b>Added by:</b>
  <%= @guideline.user.profile_name %>

</p>
<% end %>

<p>Created on <%= @guideline.created_at.strftime("%d %b %Y") %></p>





<% if @guideline.user == current_user %>
<%= link_to 'Edit', edit_guideline_path(@guideline) %> |
<%= link_to 'Back', guidelines_path %>
<% end %>